{
  "name": "root",
  "private": true,
  "scripts": {},
  "husky": {
    "pre-commit": "cd cloudcannon-snippets && npm run build"
  },
  "devDependencies": {
    "@semantic-release/git": "^9.0.0",
    "lerna": "^3.22.1",
    "magic-memo": "^1.0.3"
  },
  "dependencies": {
    "husky": "^4.3.5",
    "yargs-parser": "^20.2.1"
  },
  "release": {
    "branches": "master",
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/git",
        {
          "assets": [
            "dist/**/*.{js,css}",
            "docs",
            "package.json"
          ],
          "message": "chore(release): ${nextRelease.version} [skip ci]\n\n${nextRelease.notes}"
        }
      ]
    ]
  }
}
